[32m+[m[32m// Helper functions[m
[32m+[m[32mexport const getCarById = (id: string) => realCars.find(car => car.id === id);[m
[32m+[m[32mexport const getServiceById = (id: string) => realCarServices.find(service => service.id === id);[m
[32m+[m[32mexport const getPartById = (id: string) => realCarParts.find(part => part.id === id);[m
[32m+[m
[32m+[m[32mexport const formatPrice = (price: string) => {[m
[32m+[m[32m  return price.replace(/(\d+),?(\d+)?\s*ÿ¨ŸÜŸäŸá/, (match, p1, p2) => {[m
[32m+[m[32m    const number = p2 ? `${p1},${p2}` : p1;[m
[32m+[m[32m    return `${number} ÿ¨.ŸÖ`;[m
[32m+[m[32m  });[m
[32m+[m[32m};[m
