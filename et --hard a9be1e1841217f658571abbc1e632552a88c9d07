[32m+[m[32mexport const categories = {[m
[32m+[m[32m  cars: {[m
[32m+[m[32m    name: 'السيارات',[m
[32m+[m[32m    description: 'سيارات جديدة ومستعملة بأفضل الأسعار',[m
