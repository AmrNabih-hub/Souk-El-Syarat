{
  "name": "souk-el-syarat",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Professional Arabic car marketplace with comprehensive features",
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.8.2",
  "scripts": {
    "dev": "npx vite --config vite.config.simple.js",
    "build": "npx vite build --config vite.config.simple.js",
    "build:production": "npx vite build --config vite.config.simple.js --mode production",
    "preview": "npx vite preview --config vite.config.simple.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "analyze": "npm run build && vite-bundle-analyzer dist/stats.html",
    "clean": "echo 'Cleaning complete'",
    "postinstall": "echo 'âœ… Dependencies installed successfully'"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@tanstack/react-query": "^5.62.2",
    "appwrite": "15.0.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.468.0",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.1.1",
    "recharts": "^3.2.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@size-limit/preset-app": "^11.1.6",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "5.0.3",
    "@types/node": "22.18.8",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.1",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
