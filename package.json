{
  "name": "souk-el-syarat",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Professional Arabic car marketplace with comprehensive features",
  "engines": {
    "node": ">=20.17.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.8.2",
  "scripts": {
    "dev": "vite --port 5000",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "build:ci": "vite build",
    "preview": "vite preview --port 5000",
    "deploy": "node scripts/deploy-production.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "node scripts/deploy-production.js netlify",
    "init-supabase": "node scripts/init-supabase.js",
    "verify": "node scripts/verify-production.cjs",
    "verify:build": "node scripts/verify-production.cjs && npm run build",
    "validate:supabase": "tsx scripts/validate-supabase-integration.ts",
    "test:db": "tsx scripts/test-database-operations.ts",
    "setup:supabase": "tsx scripts/setup-supabase-complete.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:ci": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:ci": "tsc --noEmit --skipLibCheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run src/__tests__/unit",
    "test:integration": "vitest run src/__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "analyze": "echo 'Bundle analysis not configured yet' && exit 0",
    "clean": "echo 'Cleaning complete'",
    "postinstall": "echo 'âœ… Dependencies installed successfully'"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.10",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@supabase/auth-helpers-react": "0.5.0",
    "@supabase/auth-ui-react": "0.4.7",
    "@supabase/auth-ui-shared": "0.1.8",
    "@supabase/postgrest-js": "1.21.4",
    "@supabase/realtime-js": "2.15.6",
    "@supabase/storage-js": "2.12.2",
    "@supabase/supabase-js": "2.58.0",
    "@tanstack/react-query": "^5.62.2",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.468.0",
    "postcss": "^8.4.47",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.54.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.1.1",
    "recharts": "^3.2.0",
    "tsx": "4.20.6",
    "yup": "^1.6.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@size-limit/preset-app": "^11.1.6",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "5.0.3",
    "@types/node": "24.6.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.1",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "10.4.21",
    "c8": "^10.1.3",
    "complexity-report": "^2.0.0-alpha",
    "coverage-badges-cli": "^2.0.1",
    "cross-env": "7.0.3",
    "dotenv": "17.2.3",
    "eslint": "8.57.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "jsdom": "^26.0.0",
    "lighthouse": "^12.3.0",
    "msw": "^2.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.7",
    "size-limit": "^11.1.6",
    "stylelint": "16.24.0",
    "stylelint-config-standard": "39.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "supabase": "2.48.3",
    "tailwindcss": "3.4.17",
    "terser": "5.44.0",
    "typedoc": "^0.27.6",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-bundle-analyzer": "^0.12.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "1.0.3",
    "vitest": "^2.1.8",
    "workbox-window": "7.3.0"
  },
  "overrides": {
    "postcss": "^8.4.47",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "ecommerce",
    "marketplace",
    "automotive",
    "egypt",
    "cars",
    "react",
    "typescript"
  ],
  "author": "Souk El-Syarat Team",
  "license": "MIT"
}
