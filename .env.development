# ðŸ”§ Development Environment Variables
# These are safe defaults for local development
# Override these in .env.local with your actual Supabase credentials

VITE_APP_ENV=development
VITE_APP_NAME=Souk El-Sayarat
VITE_APP_URL=http://localhost:5000

# Feature Flags (Development)
VITE_ENABLE_REALTIME=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=true
VITE_ENABLE_EMAIL_NOTIFICATIONS=false

# Debug Mode
VITE_DEBUG=true

# Development Settings
VITE_API_RATE_LIMIT=1000
VITE_SESSION_TIMEOUT=120
VITE_MAX_UPLOAD_SIZE=10

# Mock Mode (if Supabase not configured)
VITE_USE_MOCK_DATA=false
